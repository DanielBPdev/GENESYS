<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  
  <parent>
    <groupId>com.asopagos.coreaas.database</groupId>
    <artifactId>database-resources</artifactId>
    <version>0.0.2-SNAPSHOT</version>
  </parent>
  
  <artifactId>database-commons</artifactId>
  <name>database-commons artifacts</name>
  
	<properties>
		<changeLogFile>target/classes/master.xml</changeLogFile>
	</properties>
  
  <dependencies>
	<dependency>
		<groupId>com.microsoft.sqlserver</groupId>
		<artifactId>mssql-jdbc</artifactId>
		<version>${sqlserver.driver}</version>
	</dependency>
  </dependencies>
	
	  <build>
      <plugins>
		<plugin>
		   <groupId>org.liquibase</groupId>
		   <artifactId>liquibase-maven-plugin</artifactId>
		   <configuration>
			  <propertyFileWillOverride>true</propertyFileWillOverride>
			  <promptOnNonLocalDatabase>false</promptOnNonLocalDatabase>
		   </configuration>                
		</plugin>
      </plugins>
    </build>
  
  <packaging>pom</packaging>
	<modules>
		<module>database-commons-core</module>
		<module>database-commons-subsidios</module>
		<module>database-commons-reportes</module>
	</modules>

</project>
