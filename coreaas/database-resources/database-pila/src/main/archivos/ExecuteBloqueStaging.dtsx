	SELECT per.perTipoIdentificacion, per.perNumeroIdentificacion, sue.sueCodigo, sue.sueNombre, sueSucursalPrincipal, 999999 AS IdTransaccion
	FROM dbo.SucursalEmpresa sue
	INNER JOIN dbo.Empresa emp ON sue.sueEmpresa = emp.empId
	INNER JOIN dbo.Persona per ON emp.empPersona = per.perId	
