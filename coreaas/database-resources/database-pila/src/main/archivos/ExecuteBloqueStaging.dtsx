      <DTS:VariableValue
        DTS:DataType="8">DECLARE @Porcentajes AS TABLE (Tipo VARCHAR(30), Clasificacion VARCHAR(100), Valor DECIMAL(5,5));

DECLARE @Parametro AS TABLE (prmNombre VARCHAR(100), prmValor VARCHAR(150))

INSERT INTO @Parametro (prmNombre, prmValor) 
SELECT prmNombre, 
	CAST(CASE 
		WHEN prmValor LIKE '%s' THEN CAST(REPLACE(prmValor, 's', ') AS BIGINT) * 1000
		WHEN prmValor LIKE '%m' THEN CAST(REPLACE(prmValor, 'm', ') AS BIGINT) * 60 * 1000
		WHEN prmValor LIKE '%h' THEN CAST(REPLACE(prmValor, 'h', ') AS BIGINT) * 60 * 60 * 1000
		WHEN prmValor LIKE '%d' THEN CAST(REPLACE(prmValor, 'd', ') AS BIGINT) * 24 * 60 * 60 * 1000
		ELSE prmValor
	END AS VARCHAR(150))
FROM dbo.Parametro WHERE prmNombre = 'TIEMPO_REINTEGRO_GF'

INSERT INTO @Porcentajes (Tipo,Clasificacion,Valor)
VALUES 
	('TRABAJADOR_DEPENDIENTE','TRABAJADOR_DEPENDIENTE', 0),
	('TRABAJADOR_INDEPENDIENTE','TRABAJADOR_INDEPENDIENTE_2_POR_CIENTO', 0.02),
	('TRABAJADOR_INDEPENDIENTE','TRABAJADOR_INDEPENDIENTE_0_6_POR_CIENTO', 0.006),
	('PENSIONADO','FIDELIDAD_25_ANIOS', 0),
	('PENSIONADO','MENOS_1_5_SM_0_POR_CIENTO', 0),
	('PENSIONADO','MENOS_1_5_SM_0_6_POR_CIENTO', 0.006),
	('PENSIONADO','MENOS_1_5_SM_2_POR_CIENTO', 0.02),
	('PENSIONADO','MAS_1_5_SM_0_6_POR_CIENTO', 0.006),
	('PENSIONADO','MAS_1_5_SM_2_POR_CIENTO', 0.02),
	('PENSIONADO','PENSION_FAMILIAR', 0);


WITH cteEstadosCotizante AS
(

	--TipoCotizante: DEPENDIENTE

	SELECT
	roaId,vw.perId,roaAfiliado,roaSucursalEmpleador,appPeriodoAporteYYYYMM	
	,appTipoIdentificacionCotizante
	,appNumeroIdentificacionCotizante
	,appTipoIdentificacionAportante
	,appNumeroIdentificacionAportante
	,RolAfiliado.roaClaseTrabajador,RolAfiliado.roaTipoAfiliado,appTipoCotizante
	,RolAfiliado.roaFechaAfiliacion, RolAfiliado.roaFechaRetiro
	,vw.roaEstadoAfiliado,RolAfiliado.roaMotivoDesafiliacion, RolAfiliado.roaPorcentajePagoAportes
	FROM pila.Aporte app 
	INNER JOIN VW_EstadoAfiliacionPersonaEmpresa vw 
						ON	vw.perTipoIdentificacion = app.appTipoIdentificacionCotizante 
						AND vw.perNumeroIdentificacion = app.appNumeroIdentificacionCotizante
						AND vw.perTipoIdentificacionEmpleador = app.appTipoIdentificacionAportante 
						AND vw.perNumeroIdentificacionEmpleador = app.appNumeroIdentificacionAportante	
	INNER JOIN Empresa ON vw.perIdEmpleador = Empresa.empPersona
	LEFT JOIN (	Afiliado 
				INNER JOIN RolAfiliado ON Afiliado.afiId = RolAfiliado.roaAfiliado
				INNER JOIN Empleador ON RolAfiliado.roaEmpleador = Empleador.empId) ON vw.perId = Afiliado.afiPersona AND Empresa.empId = Empleador.empEmpresa
	WHERE app.appTransaccion = 999999
	AND app.appTipoCotizante = 'TRABAJADOR_DEPENDIENTE'
	
	--INDEPENDIENTES PENSIONADOS

	UNION ALL

	SELECT
	roaId,vw.perId,roaAfiliado,roaSucursalEmpleador,appPeriodoAporteYYYYMM	
	,appTipoIdentificacionCotizante
	,appNumeroIdentificacionCotizante
	,appTipoIdentificacionAportante
	,appNumeroIdentificacionAportante
	,RolAfiliado.roaClaseTrabajador,RolAfiliado.roaTipoAfiliado,appTipoCotizante
	,RolAfiliado.roaFechaAfiliacion, RolAfiliado.roaFechaRetiro
	,vw.roaEstadoAfiliado,RolAfiliado.roaMotivoDesafiliacion, RolAfiliado.roaPorcentajePagoAportes
	FROM pila.Aporte app 
	INNER JOIN VW_EstadoAfiliacionPersonaIndependiente vw ON vw.perTipoIdentificacion = app.appTipoIdentificacionCotizante 
						AND vw.perNumeroIdentificacion = app.appNumeroIdentificacionCotizante
	LEFT JOIN Afiliado ON vw.perId = Afiliado.afiPersona
	LEFT JOIN RolAfiliado ON Afiliado.afiId = RolAfiliado.roaAfiliado AND app.appTipoCotizante = RolAfiliado.roaTipoAfiliado
	WHERE app.appTransaccion = 999999
	AND app.appTipoCotizante IN ('TRABAJADOR_INDEPENDIENTE')

	-- PENSIONADOS
	UNION ALL

	SELECT
	roaId,vw.perId,roaAfiliado,roaSucursalEmpleador,appPeriodoAporteYYYYMM	
	,appTipoIdentificacionCotizante
	,appNumeroIdentificacionCotizante
	,appTipoIdentificacionAportante
	,appNumeroIdentificacionAportante
	,RolAfiliado.roaClaseTrabajador,RolAfiliado.roaTipoAfiliado,appTipoCotizante
	,RolAfiliado.roaFechaAfiliacion, RolAfiliado.roaFechaRetiro
	,vw.roaEstadoAfiliado,RolAfiliado.roaMotivoDesafiliacion, RolAfiliado.roaPorcentajePagoAportes
	FROM pila.Aporte app 
	INNER JOIN VW_EstadoAfiliacionPersonaPensionado vw ON vw.perTipoIdentificacion = app.appTipoIdentificacionCotizante 
						AND vw.perNumeroIdentificacion = app.appNumeroIdentificacionCotizante
	LEFT JOIN Afiliado ON vw.perId = Afiliado.afiPersona
	LEFT JOIN RolAfiliado ON Afiliado.afiId = RolAfiliado.roaAfiliado AND app.appTipoCotizante = RolAfiliado.roaTipoAfiliado
	WHERE app.appTransaccion = 999999
	AND app.appTipoCotizante IN ('PENSIONADO')
	
)

 SELECT DISTINCT appTipoIdentificacionCotizante perTipoIdentificacion, appNumeroIdentificacionCotizante perNumeroIdentificacion,
	appTipoIdentificacionAportante empTipoIdentificacion,
	appNumeroIdentificacionAportante empNumeroIdentificacion
	,PersonaDetalle.pedFechaFallecido,
	CASE 
		WHEN cte.appTipoCotizante = 'TRABAJADOR_DEPENDIENTE' THEN 0 
		WHEN cte.appTipoCotizante != 'TRABAJADOR_DEPENDIENTE' AND cte.roaPorcentajePagoAportes IS NOT NULL THEN cte.roaPorcentajePagoAportes 
			ELSE p.Valor 
	END PorcentajePagoAportes,cte.roaEstadoAfiliado AS roaEstadoAfiliado
	,cte.roaClaseTrabajador,appTipoCotizante AS roaTipoAfiliado
	,SucursalEmpresa.sueCodigo, SucursalEmpresa.sueNombre,cte.roaFechaAfiliacion, cte.roaFechaRetiro, ISNULL(snp.FechaUltimaNovedad, cte.roaFechaRetiro) FechaUltimaNovedad
	,CASE WHEN ((ISNULL(cte.roaEstadoAfiliado,'') IN ('INACTIVO','NO_FORMALIZADO_RETIRADO_CON_APORTES') AND ISNULL(cte.roaMotivoDesafiliacion,'') NOT IN ('FALLECIMIENTO', 'AFILIACION_ANULADA')) OR (ISNULL(epc.roaEstadoAfiliado, '') IN ('INACTIVO','NO_FORMALIZADO_RETIRADO_CON_APORTES'))
		OR (ISNULL(cte.roaEstadoAfiliado,'') IN ('INACTIVO','NO_FORMALIZADO_RETIRADO_CON_APORTES') AND EXISTS (SELECT 1
			FROM Afiliado 
			INNER JOIN RolAfiliado ON RolAfiliado.roaAfiliado = Afiliado.afiId
			WHERE Afiliado.afiPersona = cte.perId
				AND ISNULL(RolAfiliado.roaMotivoDesafiliacion,'') NOT IN ('FALLECIMIENTO', 'AFILIACION_ANULADA')))
	) THEN 1 ELSE 0 END AS EsTrabajadorReintegrable
	, CASE WHEN epc.roaFechaRetiro IS NOT NULL AND DATEDIFF(day, epc.roaFechaRetiro, dbo.GetLocalDate()) &lt;= CAST(ParamTR.prmValor AS BIGINT)/86400000 
		THEN 1 ELSE 0 END AS grupoFamiliarReintegrable
	,cte.appPeriodoAporteYYYYMM
	, 999999 AS IdTransaccion
	FROM cteEstadosCotizante cte
	INNER JOIN VW_EstadoAfiliacionPersonaCaja epc ON epc.perId = cte.perId
	CROSS JOIN @Parametro ParamTR
	LEFT JOIN (	SELECT MAX(sapSolicitudGlobal) sapSolicitudGlobal, sapRolAfiliado
				FROM SolicitudAfiliacionPersona
				WHERE sapEstadoSolicitud &lt;&gt; 'PRE_RADICADA'
				GROUP BY sapRolAfiliado ) sap ON cte.roaId = sapRolAfiliado
	LEFT JOIN Solicitud sol ON sap.sapSolicitudGlobal = sol.solId
	LEFT JOIN @Porcentajes p ON cte.roaTipoAfiliado = p.Tipo AND sol.solClasificacion = p.Clasificacion
	LEFT JOIN PersonaDetalle ON cte.perId = PersonaDetalle.pedPersona
	LEFT JOIN (
		SELECT snp.snpRolAfiliado, MAX(ISNULL(nop.nopFechaFin, ISNULL(nop.nopFechaInicio, sol.solFechaRadicacion))) FechaUltimaNovedad
		FROM Solicitud sol
		INNER JOIN SolicitudNovedad sno ON sol.solId = sno.snoId
		INNER JOIN SolicitudNovedadPersona snp ON sno.snoId = snp.snpSolicitudNovedad
		INNER JOIN NovedadDetalle nop ON sno.snoId = nop.nopSolicitudNovedad
		WHERE sol.solResultadoProceso = 'APROBADA'
		GROUP BY snp.snpRolAfiliado
	) snp ON snp.snpRolAfiliado = cte.roaAfiliado
	LEFT JOIN SucursalEmpresa ON cte.roaSucursalEmpleador = SucursalEmpresa.sueId
	WHERE ParamTR.prmNombre = 'TIEMPO_REINTEGRO_GF'</DTS:VariableValue>
    </DTS:Variable>
