		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>archivos-pila-api</artifactId>
			<version>${project.version}</version>
			<exclusions>
				<exclusion>
					<groupId>com.heinsohn.lion.components.fileProcessing</groupId>
					<artifactId>lion-fileProcessingHot</artifactId>
				</exclusion>
				<exclusion>
					<groupId>com.heinsohn.lion.components.common</groupId>
					<artifactId>lion-commonHot</artifactId>
				</exclusion>
				<exclusion>
					<groupId>com.heinsohn.lion.components.cache</groupId>
					<artifactId>lion-cacheHot</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>entidad-descuento-composite-api</artifactId>
