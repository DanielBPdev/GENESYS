	<artifactId>process-schedule-service</artifactId>
	<packaging>war</packaging>
	<name>Process Schedule Service</name>
	<description>Aplicación Web que implementa los procesos automáticos</description>

	<properties>
		<fabric8.skip>false</fabric8.skip>
	</properties>

	<dependencies>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>novedades-composite-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>process-schedule-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>usuarios-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>subsidio-monetario-api</artifactId>
