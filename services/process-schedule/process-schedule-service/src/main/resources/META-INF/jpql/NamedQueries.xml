    <named-query name="ProcessSchedule.ParametrizacionEjecucionProgramada.buscarPorTipoTransaccion">
		<description>Busca la parametrización para la Ejecución Programada por Tipo de Transacción.</description>
		<query>
			SELECT pep 
			FROM ParametrizacionEjecucionProgramada pep 
			WHERE pep.proceso =:proceso
        </query>
	</named-query>
	<!-- Actualizacion bloque 9 id plantillas -->
	<named-query
		name="Parametrizacion.Programacion.consultarProgramacionPorProcesos">
		<description>Busca la parametrización de progamacion por procesos</description>
		<query>
			SELECT pejep 
			FROM ParametrizacionEjecucionProgramada pejep
			WHERE pejep.proceso IN :procesos
        </query>
	</named-query>

	<named-query name="ProcessSchedule.resultadoEjecucionProgramada.buscarPorProceso">
		<description>Busca la parametrización para la Ejecución Programada por Tipo de Transacción.</description>
		<query>
			SELECT rep
			FROM ResultadoEjecucionProgramada rep
			WHERE rep.proceso =:proceso and rep.tipoResultadoProceso =:resultadoProceso
			ORDER BY rep.idResultadoEjecucion DESC
		</query>
	</named-query>

<!-- Actualizacion bloque 9 id plantillas -->
<named-native-query
name="Parametrizacion.Programacion.liberarPlanillasBloque9">
<description>Consulta los id de planillas con pendiente en bloque 9 , para liberarcion</description>
<query>

	SELECT  pipId FROM  PilaIndicePlanilla pila
	WHERE  pila.pipEstadoArchivo in ('PROCESADO_SIN_NOVEDADES', 'PROCESADO_NOVEDADES')
	AND  pila.pipTipoArchivo like 'ARCHIVO_OI_I%'
</query>
</named-native-query>


