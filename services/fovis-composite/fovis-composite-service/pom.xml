<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>com.asopagos.coreaas.services</groupId>
		<artifactId>fovis-composite</artifactId>
		<version>0.0.2-SNAPSHOT</version>
	</parent>

	<artifactId>fovis-composite-service</artifactId>
	<packaging>war</packaging>
	<name>FOVIS Composite Service</name>
	<description>Aplicación Web que implementa los servicios backend de fovis-composite-service</description>

	<properties>
		<configuracion.servidor>wildfly-swarm-fovis</configuracion.servidor>
		<fabric8.skip>false</fabric8.skip>
	</properties>

	<dependencies>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>fovis-composite-api</artifactId>
			<version>${project.version}</version>
		</dependency> 
		
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>fovis-api</artifactId>
			<version>${project.version}</version>
		</dependency>   
		
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>novedades-fovis-api</artifactId>
			<version>${project.version}</version>
		</dependency>   
		
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>personas-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>process-schedule-api</artifactId>
			<version>${project.version}</version>
		</dependency>

		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>empleadores-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>empresas-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>afiliados-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>afiliaciones-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>validaciones-api</artifactId>
			<version>${project.version}</version>
		</dependency>

		<dependency>
			<groupId>com.fasterxml.jackson.core</groupId>
			<artifactId>jackson-databind</artifactId>
		</dependency>
		
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>solicitudes-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>validaciones-fovis-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>listas-chequeo-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>asignar-solicitudes-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
            <groupId>com.asopagos.coreaas.services</groupId>
            <artifactId>archivos-api</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.asopagos.coreaas.services</groupId>
            <artifactId>consola-estado-cargue-procesos-api</artifactId>
            <version>${project.version}</version>
        </dependency> 
		
		<dependency>
			<groupId>javax.enterprise.concurrent</groupId>
			<artifactId>javax.enterprise.concurrent-api</artifactId>
			<version>1.0</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
            <groupId>com.asopagos.coreaas.services</groupId>
            <artifactId>consola-ejecucion-proceso-asincrono-api</artifactId>
            <version>${project.version}</version>
        </dependency> 
        <dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>legalizacion-fovis-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>cartera-api</artifactId>
			<version>${project.version}</version>
		</dependency>
        <dependency>
            <groupId>com.asopagos.coreaas.services</groupId>
            <artifactId>listas-api</artifactId>
            <version>${project.version}</version>
        </dependency>
        
        <!-- API para fórmulas dinámicas -->
        <dependency>
            <groupId>com.asopagos.coreaas</groupId>
            <artifactId>utilities-formulador</artifactId>
            <version>${project.version}</version>
        </dependency>
	</dependencies>

	<build>
		<!-- Maven will append the version to the finalName (which is the name 
			given to the generated war, and hence the context root) -->
		<finalName>FovisCompositeService</finalName>
		<plugins>
			<plugin>
				<groupId>org.wildfly.plugins</groupId>
				<artifactId>wildfly-maven-plugin</artifactId>
				<configuration>
					<skip>false</skip>
				</configuration>
			</plugin>
		</plugins>
	</build>

	<profiles>
	</profiles>

</project>
