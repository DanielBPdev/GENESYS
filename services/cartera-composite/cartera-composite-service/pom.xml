<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>com.asopagos.coreaas.services</groupId>
        <artifactId>cartera-composite</artifactId>
        <version>0.0.2-SNAPSHOT</version>
    </parent>

    <artifactId>cartera-composite-service</artifactId>
    <packaging>war</packaging>
    <name>Servicio Cartera Composite</name>
    <description>Aplicaci√≥n Web que implementa los servicios backend de cartera</description>

    <properties>
        <configuracion.servidor>wildfly-swarm-cartera</configuracion.servidor>
        <fabric8.skip>false</fabric8.skip>
    </properties>

    <dependencies>
        <dependency>
            <groupId>com.asopagos.coreaas.services</groupId>
            <artifactId>cartera-composite-api</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.asopagos.coreaas.services</groupId>
            <artifactId>cartera-api</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.asopagos.coreaas.services</groupId>
            <artifactId>tareas-humanas-api</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.asopagos.coreaas.services</groupId>
            <artifactId>usuarios-api</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.asopagos.coreaas.services</groupId>
            <artifactId>personas-api</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.asopagos.coreaas.services</groupId>
            <artifactId>afiliaciones-api</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.asopagos.coreaas.services</groupId>
            <artifactId>asignar-solicitudes-api</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.asopagos.coreaas.services</groupId>
            <artifactId>solicitudes-api</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.asopagos.coreaas.services</groupId>
            <artifactId>afiliados-api</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.asopagos.coreaas.services</groupId>
            <artifactId>process-schedule-api</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.asopagos.coreaas.services</groupId>
            <artifactId>empresas-api</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.asopagos.coreaas.services</groupId>
            <artifactId>archivos-pila-api</artifactId>
            <version>${project.version}</version>
            <exclusions>
                <exclusion>
                    <groupId>com.heinsohn.lion.components.fileProcessing</groupId>
                    <artifactId>lion-fileProcessingHot</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>com.heinsohn.lion.components.common</groupId>
                    <artifactId>lion-commonHot</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>com.heinsohn.lion.components.cache</groupId>
                    <artifactId>lion-cacheHot</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>com.fasterxml.jackson.core</groupId>
            <artifactId>jackson-databind</artifactId>
        </dependency>
        <dependency>
            <groupId>com.asopagos.coreaas.services</groupId>
            <artifactId>listas-api</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.asopagos.coreaas.services</groupId>
            <artifactId>historicos-api</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.asopagos.coreaas.services</groupId>
            <artifactId>reportes-api</artifactId>
