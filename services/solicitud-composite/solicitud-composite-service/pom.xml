		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>afiliacion-personas-web-api</artifactId>
			<version>${project.version}</version>
		</dependency>   
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>fovis-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
