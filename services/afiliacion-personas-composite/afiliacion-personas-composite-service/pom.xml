<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>com.asopagos.coreaas.services</groupId>
		<artifactId>afiliacion-personas-composite</artifactId>
		<version>0.0.2-SNAPSHOT</version>
	</parent>

	<artifactId>afiliacion-personas-composite-service</artifactId>
	<packaging>war</packaging>
	<name>Servicio Afiliación Personas Composite</name>
	<description>Aplicación Web que implementa los servicios backend de afiliación de personas</description>

	<properties>
		<fabric8.skip>false</fabric8.skip>
	</properties>

	<dependencies>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>afiliacion-personas-composite-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>asignar-solicitudes-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>empleadores-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>empresas-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>listas-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>novedades-composite-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>afiliaciones-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>consola-estado-cargue-procesos-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>solicitudes-api</artifactId>
			<version>${project.version}</version>
		</dependency><dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>historicos-api</artifactId>
			<version>${project.version}</version>
		</dependency>
	</dependencies>

	<build>
		<!-- Maven will append the version to the finalName (which is the name 
			given to the generated war, and hence the context root) -->
		<finalName>AfiliacionPersonasCompositeService</finalName>
		<plugins>
			<plugin>
				<groupId>org.wildfly.plugins</groupId>
				<artifactId>wildfly-maven-plugin</artifactId>
				<configuration>
					<skip>false</skip>
				</configuration>
			</plugin>
		</plugins>
	</build>

	<profiles>
	</profiles>

</project>