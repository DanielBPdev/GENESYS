<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>com.asopagos.coreaas.services</groupId>
		<artifactId>afiliados</artifactId>
		<version>0.0.2-SNAPSHOT</version>
	</parent>
	<artifactId>afiliados-service</artifactId>
	<packaging>war</packaging>
	<name>Servicio Afiliados</name>
	<description>Aplicacion Web que implementa los servicios backend de afiliados</description>

	<properties>
	    <base.servidor>wildfly-swarm-ee</base.servidor>
		<configuracion.servidor>wildfly-swarm-afiliados</configuracion.servidor>
		<fabric8.skip>false</fabric8.skip>
	</properties>

	<dependencies>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>afiliados-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.model</groupId>
			<artifactId>model-fovis-entities</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>commons-beanutils</groupId>
			<artifactId>commons-beanutils</artifactId>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.services</groupId>
			<artifactId>cliente-anibol-api</artifactId>
			<version>${project.version}</version>
		</dependency>
		
		<!-- Rutinas -->
		
		<dependency>
			<groupId>com.asopagos.coreaas.rutines.afiliados-rutines</groupId>
			<artifactId>actualizar-novedad-pila-rutine</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.rutines.afiliados-rutines</groupId>
			<artifactId>deshacer-gestion-cero-trabajadores-rutine</artifactId>
			<version>${project.version}</version>
		</dependency>		
		<dependency>
			<groupId>com.asopagos.coreaas.rutines.afiliados-rutines</groupId>
			<artifactId>inactivar-beneficiario-rol-afiliado-rutine</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.rutines.afiliados-rutines</groupId>
			<artifactId>actualizar-rol-afiliado-rutine</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.rutines.afiliados-rutines</groupId>
			<artifactId>actualizar-datos-afiliado-rutine</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.rutines.afiliados-rutines</groupId>
			<artifactId>actualizar-medio-de-pago-grupo-familiar-rutine</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.rutines.afiliados-rutines</groupId>
			<artifactId>actualizar-grupo-familiar-persona-rutine</artifactId>
			<version>${project.version}</version>
		</dependency>		
		<dependency>
			<groupId>com.asopagos.coreaas.rutines.afiliados-rutines</groupId>
			<artifactId>calcular-categorias-afiliado-rutine</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.rutines.afiliados-rutines</groupId>
			<artifactId>actualizar-beneficiario-rutine</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.asopagos.coreaas.rutines.afiliados-rutines</groupId>
			<artifactId>consultar-datos-afiliado-rutine</artifactId>
			<version>${project.version}</version>
		</dependency>		
		<dependency>
			<groupId>com.asopagos.coreaas.rutines.afiliados-rutines</groupId>
			<artifactId>activar-afiliado-rutine</artifactId>
			<version>${project.version}</version>
		</dependency>		
		<dependency>
			<groupId>com.asopagos.coreaas.rutines.afiliados-rutines</groupId>
			<artifactId>actualizar-beneficiario-simple-rutine</artifactId>
			<version>${project.version}</version>
		</dependency>
		
	</dependencies>

	<build>
		<!-- Maven will append the version to the finalName (which is the name 
			given to the generated war, and hence the context root) -->
		<finalName>AfiliadosService</finalName>
		<plugins>
			<plugin>
				<groupId>org.wildfly.plugins</groupId>
				<artifactId>wildfly-maven-plugin</artifactId>
				<configuration>
					<skip>false</skip>
				</configuration>
			</plugin>
		</plugins>
	</build>

	<profiles>
	</profiles>
</project>
